{
  "name": "frontend-template",
  "private": true,
  "version": "0.0.0",
  "main": "index.js",
  "license": "CC-BY-4.0",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{!(*.g).ts,*.tsx}": [
      "eslint --fix",
      "stylelint --fix",
      "git add"
    ]
  },
  "scripts": {
    "build": "gatsby build",
    "start": "gatsby develop",
    "setup": "yarn install",
    "reset:modules": "rimraf node_modules/ yarn.lock apps/*/node_modules",
    "reset:yarn": "yarn cache clean",
    "reset:rn": "watchman watch-del-all; rimraf $TMPDIR/react-*; rimraf $TMPDIR/haste-map-react-native-packager-*",
    "reset:cache": "yarn reset:yarn && yarn reset:rn",
    "reset": "yarn reset:modules && yarn reset:cache"
  },
  "dependencies": {
    "gatsby": "2.20.25",
    "gatsby-plugin-typescript": "2.3.3",
    "gatsby-plugin-graphql-codegen": "2.7.1",
    "gatsby-source-strapi": "0.0.12",
    "mkdirp": "0.5.1",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "rimraf": "2.6.3",
    "typescript": "3.8.3"
  },
  "devDependencies": {
    "@types/node": "8.0.7",
    "@types/react": "15.0.21",
    "@types/react-dom": "0.14.23",
    "@typescript-eslint/eslint-plugin": "2.21.0",
    "@typescript-eslint/parser": "2.21.0",
    "concurrently": "4.1.0",
    "eslint": "6.8.0",
    "eslint-config-airbnb": "18.0.1",
    "eslint-config-prettier": "6.10.0",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.18.3",
    "husky": "3.0.5",
    "lint-staged": "9.4.0",
    "prettier": "1.19.1",
    "stylelint": "11.1.0",
    "stylelint-config-recommended": "3.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.10.0"
  }
}
